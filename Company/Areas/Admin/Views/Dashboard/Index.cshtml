				<h3>@Model.count_freelancer</h3>
				<p>Freelancer</p> 
			</span>
		</li>
		<li>
			<i class='bx bxs-dollar-circle'></i>
			<span class="text">
				<h3>@Model.count_Inprocess</h3>
				<p>In process projects</p>
			</span>
		</li>
	</ul>


	<div class="table-data">
		<div class="order">
			<div class="head">
				<h3>Recent Orders</h3>
				<i class='bx bx-search'></i>
				<i class='bx bx-filter'></i>
			</div>
			<table id="tblDataInfo">
				<thead>
					<tr>
						<th>User</th>
						<th>Date Order</th>
						<th>Status</th>
					</tr>
				</thead>
				<tbody>
					@foreach (var item in Model.ProjectsList)
					{
						<tr>
							<td>
								<img src="~/img/users/user1.jpg">
								<p>@item.Name</p>
							</td>
							<td>@item.CreationDate</td>
							<td>
								@if(item.status==SD.ProjectFinish){
									<span class="status completed bg-success">Completed</span>
								}
								else if(item.status==SD.ProjectNotAssigned)
								{
									<span class="status pending bg-danger">Not Assigned</span>
								}
								else if (item.status == SD.ProjectWaitingResponse)
								{
									<span class="status pending">Waiting Response</span>
								}
								else
								{
									<span class="status process">@(item.status ?? "No status")</span>
								}

							</td>
						</tr>
					}
